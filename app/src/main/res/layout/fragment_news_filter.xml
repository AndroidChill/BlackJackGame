<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:navigationIcon="@drawable/ic_baseline_arrow_back_ios_24"
            app:title="Фильтр" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip3"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginStart="16dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip4"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="@+id/chip3"
            app:layout_constraintTop_toBottomOf="@+id/chip3" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip5"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="@+id/chip4"
            app:layout_constraintTop_toBottomOf="@+id/chip4" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip6"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="@+id/chip5"
            app:layout_constraintTop_toBottomOf="@+id/chip5" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip7"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="@+id/chip6"
            app:layout_constraintTop_toBottomOf="@+id/chip6" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip8"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:height="48dp"
            android:theme="@style/Theme.MaterialComponents.Light"
            style="@style/Widget.MaterialComponents.Chip.Choice"
            app:layout_constraintStart_toStartOf="@+id/chip6"
            app:layout_constraintTop_toBottomOf="@+id/chip7" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Обновление"
            app:layout_constraintBottom_toBottomOf="@+id/chip3"
            app:layout_constraintStart_toEndOf="@+id/chip3"
            app:layout_constraintTop_toTopOf="@+id/chip3" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Информация о ваших турнирах"
            app:layout_constraintBottom_toBottomOf="@+id/chip4"
            app:layout_constraintStart_toEndOf="@+id/chip4"
            app:layout_constraintTop_toTopOf="@+id/chip4"
            app:layout_constraintVertical_bias="0.38" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Приглашение друзей в игру"
            app:layout_constraintBottom_toBottomOf="@+id/chip5"
            app:layout_constraintStart_toEndOf="@+id/chip5"
            app:layout_constraintTop_toTopOf="@+id/chip5"
            app:layout_constraintVertical_bias="0.38" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Заявки в друзья"
            app:layout_constraintBottom_toBottomOf="@+id/chip6"
            app:layout_constraintStart_toEndOf="@+id/chip6"
            app:layout_constraintTop_toTopOf="@+id/chip6"
            app:layout_constraintVertical_bias="0.38" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Информация о рефералах"
            app:layout_constraintBottom_toBottomOf="@+id/chip7"
            app:layout_constraintStart_toEndOf="@+id/chip7"
            app:layout_constraintTop_toTopOf="@+id/chip7"
            app:layout_constraintVertical_bias="0.38" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Результаты тунира"
            app:layout_constraintBottom_toBottomOf="@+id/chip8"
            app:layout_constraintStart_toEndOf="@+id/chip8"
            app:layout_constraintTop_toTopOf="@+id/chip8"
            app:layout_constraintVertical_bias="0.38" />

        <Button
            android:id="@+id/agree"
            style="@style/Widget.MaterialComponents.Button"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/agree"
            android:textAllCaps="false"
            android:theme="@style/Theme.MaterialComponents.Light"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/chip8" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>